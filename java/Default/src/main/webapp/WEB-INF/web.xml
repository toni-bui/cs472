		http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<servlet>
		<servlet-name>Hello</servlet-name>
		<servlet-class>edu.miu.cs472.controllers.Hello</servlet-class>
	</servlet>
	<servlet-mapping>
